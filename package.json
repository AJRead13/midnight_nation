{
  "name": "midnight-nation",
  "version": "1.0.0",
  "description": "MERN application for Midnight Nation TTRPG",
  "scripts": {
    "install-all": "cd server && npm install && cd ../client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "ttrpg",
    "mern",
    "react",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.1",
    "node": "^25.2.0"
  }
}
